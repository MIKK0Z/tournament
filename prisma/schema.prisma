generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Player {
  id        String    @id @default(uuid())
  createdAt DateTime  @default(now()) @db.Timestamptz(3)
  name      String
  surname   String
  age       Int
  city      String
}